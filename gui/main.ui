<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Toplevel" id="root">
    <property name="geometry">1500x800</property>
    <property name="height">1500</property>
    <property name="padx">0</property>
    <property name="width">800</property>
    <child>
      <object class="tk.Frame" id="main">
        <property name="height">800</property>
        <property name="padx">0</property>
        <property name="pady">1</property>
        <property name="width">1500</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.Frame" id="left">
            <property name="background">#ffffff</property>
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="tk.Frame" id="nav">
                <property name="background">#c0c0c0</property>
                <property name="height">200</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
                <child>
                  <object class="tk.Button" id="startbt">
                    <property name="cursor">arrow</property>
                    <property name="font">{TkHeadingFont,} 10 {}</property>
                    <property name="justify">center</property>
                    <property name="text" translatable="yes">Start</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="ipady">10</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Button" id="stopbt">
                    <property name="font">{TkHeadingFont,} 10 {}</property>
                    <property name="repeatdelay">0</property>
                    <property name="text" translatable="yes">Stop</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="ipady">10</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Separator" id="separator_1">
                    <property name="orient">horizontal</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="pady">10</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Button" id="exportbt">
                    <property name="default">normal</property>
                    <property name="font">{TkHeadingFont,} 10 {}</property>
                    <property name="pady">0</property>
                    <property name="text" translatable="yes">Export</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="ipady">10</property>
                      <property name="propagate">True</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Button" id="clearbt">
                    <property name="font">{TkHeadingFont,} 10 {}</property>
                    <property name="text" translatable="yes">Clear</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="ipady">10</property>
                      <property name="propagate">True</property>
                      <property name="row">4</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="tk.Frame" id="inputframe">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="dataframe">
            <property name="background">#c0c0c0</property>
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="ttk.Treeview" id="datatable">
                <property name="selectmode">extended</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Scrollbar" id="scrollbar_x">
                <property name="orient">horizontal</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Scrollbar" id="scrollbar_y">
                <property name="orient">vertical</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
